<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="pane" stylesheets="/View/style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.GamePanelController">
   <children>
      <Canvas fx:id="canvas" focusTraversable="true" height="600.0" width="1000.0" />
      <VBox fx:id="pauseMenu" alignment="CENTER" layoutX="488.0" layoutY="353.0" prefHeight="154.0" prefWidth="100.0" visible="false">
         <children>
            <Button fx:id="Continue" mnemonicParsing="false" onAction="#continueBtn" text="Continue" />
            <Button mnemonicParsing="false" onAction="#restartBtn" text="Restart" textAlignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#saveBtn" text="Save" textAlignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#loadBtn" text="Load" textAlignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#mainMenuBtn" text="Main Meny" textAlignment="CENTER" />
         </children>
      </VBox>
   </children>
</Pane>
